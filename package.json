{
  "name": "promptopia-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run setup && next dev --turbopack",
    "build": "npm run setup && next build",
    "start": "npm run setup && next start",
    "lint": "next lint",
    "setup": "node scripts/setup.js",
    "generate": "prisma generate --schema=prisma/mongodbSchema.prisma && prisma generate --schema=prisma/sqliteSchema.prisma",
    "migrate": "prisma migrate dev --schema=prisma/sqliteSchema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "dotenv": "^17.0.0",
    "jose": "^6.0.11",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "eslint": "9.29.0",
    "eslint-config-next": "15.3.3",
    "prisma": "^6.10.1",
    "tailwindcss": "^4"
  }
}
